class Solution {
    public int canCompleteCircuit(int[] gas, int[] cost) {
        int min=gas[0];
        int sum=0;
        int index=0;
        for (int i=0;i<gas.length;i++){
            sum+=gas[i]-cost[i];
            if (min>=sum){
                index=i;
                min=sum;
            }
        }
        return sum<0?-1:(index+1)%gas.length;
    }
}
