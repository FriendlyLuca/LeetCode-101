class Solution {
    public List<List<Integer>> levelOrderBottom(TreeNode root) {
        LinkedList<List<Integer>> ans=new LinkedList<List<Integer>>();//用Linked List来做前插
        Queue<TreeNode> q=new LinkedList<TreeNode>();
        if (root!=null)
            q.offer(root);
        while(!q.isEmpty()){
            int size=q.size();
            List<Integer> lvl=new ArrayList<Integer>();
            for (int i=0;i<size;i++){
                TreeNode temp=q.poll();
                if (temp.left!=null)
                    q.offer(temp.left);
                if (temp.right!=null)
                    q.offer(temp.right);
                lvl.add(temp.val);
            }
            ans.addFirst(lvl);
        }
        return ans;
    }
}
